  32   
  33  * Engineering and production
  34  capMW   = 1000;
  35  capFac  = 0.92;
  36  lifeY   = 60;
  37   
  38  * Cost inputs (OECD-NEA, already in $/MWh)
  39  capCost_MWh  = 50.32;
  40  omCost_MWh   = 11.6;
  41  fuelCost_MWh = 9.33;
  42   
  43  * Financing inputs
  44  hrs = 8760;
  45  wacc("private") = 0.08;
  46  wacc("public")  = 0.025;
  47   
  48  * Permitting / construction assumptions (PRIS cohort stats)
  49  buildY("baseline") = 14.92;
  50  buildY("reform")   = 6.44;
  51   
  52  sigY("baseline") = 10.36;
  53  sigY("reform")   = 0.66;
  54   
  55  * Risk pricing assumption
  56  kappa = 0.0075;
  57   
  58  * -------------------------
  59  * 2) DERIVED TERMS (LCOE building blocks)
  60  * -------------------------
  61  SCALARS
  62      annMWh  "annual generation (MWh/yr)";
  63   
  64  PARAMETERS
  65      rEff(o,r)    "effective WACC after risk adjustment"
  66      idcMult(o,r) "IDC-style multiplier for construction time"
  67      lcoe(o,r)    "LCOE by case ($/MWh)";
  68   
  69  annMWh = capMW * hrs * capFac;
  70   
  71  * Effective WACC: base WACC + kappa * schedule risk
  72  rEff(o,r) = wacc(o) + kappa * sigY(r);
  73   
  74  * IDC multiplier using midpoint compounding over construction time
  75  idcMult(o,r) = (1 + rEff(o,r)) ** (buildY(r)/2);
  76   
  77  * LCOE: apply IDC multiplier to capital component only, keep O&M and fuel constant
  78  lcoe(o,r) = capCost_MWh * idcMult(o,r) + omCost_MWh + fuelCost_MWh;
